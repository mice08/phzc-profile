<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://code.alibabatech.com/schema/dubbo 
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
 
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="dubbo-profile-consumer"/>
 
    <!-- 使用multicast广播注册中心暴露服务地址 -->
    <!-- <dubbo:registry address="multicast://224.5.6.7:8888" /> -->
 
 	<!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry protocol="zookeeper" address="${zk.server}" />
     	
 	<!-- <dubbo:annotation package="com.dubbo.consumer"></dubbo:annotation> -->
 	
 	<!-- 生成远程服务代理，可以和本地bean一样使用demoService -->
    <!--<dubbo:reference interface="com.phzc.messagecenter.api.dubbo.ShortMessageDubbo" id="ShortMessageDubbo"/>-->
    <dubbo:reference interface="com.phzc.pms.facade.service.dubbo.UserProductDubbo" id="userProductDubbo"/>
    <dubbo:reference interface="com.phzc.pms.facade.service.dubbo.UserRelationDubbo" id="userRelationDubbo"/>
    <dubbo:reference interface="com.phzc.pms.facade.service.dubbo.ProductDetailDubbo" id="productDetailDubbo"/>
    <dubbo:reference interface="com.phzc.pms.facade.service.dubbo.ProductProjectDetailDubbo" id="productProjectDetailDubbo"/>
    <dubbo:reference interface="com.phzc.pms.facade.service.dubbo.ProductBusinessDubbo" id="productBusinessDubbo"/>
    <!-- <dubbo:reference interface="com.phzc.wxconsole.api.service.WeixinUserService" id="weixinUserServiceDubbo" /> -->
    
</beans>