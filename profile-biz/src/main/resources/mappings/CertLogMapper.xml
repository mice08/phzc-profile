<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.phzc.profile.biz.dao.CertLogMapper">

	<resultMap id="CertLog" type="com.phzc.profile.biz.model.CertLog">
		<id column="chk_seq_id" property="chkSeqId" jdbcType="INTEGER" />
		<result column="cert_no" property="certNo" jdbcType="VARCHAR" />
		<result column="cert_name" property="certName" jdbcType="VARCHAR" />
		<result column="cert_type" property="certType" jdbcType="CHAR" />
		<result column="chk_stat" property="chkStat" jdbcType="CHAR" />
		<result column="open_datetime" property="openDatetime" jdbcType="DATE" />
		<result column="upd_datetime" property="updDatetime" jdbcType="DATE" />
	</resultMap>

    <insert id="insertCertLog" useGeneratedKeys="true" keyProperty="chkSeqId" parameterType="com.phzc.profile.biz.model.CertLog">
    	insert into `tb_cert_log` (cert_no, cert_name, cert_type, chk_stat, open_datetime, upd_datetime) values (
    		#{certNo}, 
    		#{certName}, 
    		#{certType}, 
    		#{chkStat}, 
    		now(), 
    		now()
    	)
    </insert>
    
    <update id="updateCertLog" parameterType="com.phzc.profile.biz.model.CertLog" >
        update `tb_cert_log` set 
	        cert_no = #{certNo}, 
	        cert_name = #{certName}, 
	        cert_type = #{certType}, 
	        chk_stat = #{chkStat}, 
	        upd_datetime = now() 
        where chk_seq_id = #{chkSeqId}
    </update>

</mapper> 