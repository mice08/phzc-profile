<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.phzc.profile.biz.dao.CertInfoMapper">

	<resultMap id="CertInfo" type="com.phzc.profile.biz.model.CertInfo">
		<id column="cert_no" property="certNo" jdbcType="VARCHAR" />
		<result column="cert_name" property="certName" jdbcType="VARCHAR" />
		<result column="cert_type" property="certType" jdbcType="CHAR" />
	</resultMap>

    <select id="getCertInfoByCertNo" parameterType="java.lang.String" resultMap="CertInfo">
        select * from `tb_cert_info` where cert_no = #{certNo}
    </select>
    
    <insert id="insertCertInfo" parameterType="com.phzc.profile.biz.model.CertInfo">
    	insert into `tb_cert_info` values (
    		#{certNo}, 
    		#{certName}, 
    		#{certType}
    	)
    </insert>

</mapper> 